{
  "all": true,
  "check-coverage": true,
  "reporter": [
    "html",
    "text",
    "text-summary",
    "lcov",
    "json"
  ],
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 75,
  "include": [
    "2048/**/*.js",
    "tests/**/*.js"
  ],
  "exclude": [
    "tests/**/*.test.js",
    "tests/**/test-*.js",
    "node_modules/**",
    "coverage/**",
    "test-results/**"
  ],
  "cache": true,
  "temp-dir": "./.nyc_output",
  "report-dir": "./coverage",
  "skip-full": false,
  "produce-source-map": true,
  "instrument": true,
  "source-map": true
}
